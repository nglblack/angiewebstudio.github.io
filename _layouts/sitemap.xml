---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {% for page in site.pages %}
    {% unless page.url == '/talks/' or page.url == '/publications/' or page.url == '/teaching/' or page.url == '/talkmap.html' or page.url == '/terms/' or page.url == '/year-archive/' or page.url == '/markdown_generator/' or page.url == '/sitemap/' %}
      <url>
        <loc>{{ site.url }}{{ page.url | xml_escape }}</loc>
        <lastmod>{{ page.lastmod | xml_escape }}</lastmod>
      </url>
    {% endunless %}
  {% endfor %}

  {% for post in site.posts %}
    <url>
      <loc>{{ site.url }}{{ post.url | xml_escape }}</loc>
      <lastmod>{{ post.date | date_to_xmlschema }}</lastmod>
    </url>
  {% endfor %}
</urlset>
