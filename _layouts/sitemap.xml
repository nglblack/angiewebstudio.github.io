---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {% for page in site.pages %}
    {% unless page.url contains 'talks' or page.url contains 'publications' or page.url contains 'teaching' %}
      <url>
        <loc>{{ site.url }}{{ page.url | xml_escape }}</loc>
        <lastmod>{{ page.lastmod | xml_escape }}</lastmod>
      </url>
    {% endunless %}
  {% endfor %}
  
  {% for post in site.posts %}
    <url>
      <loc>{{ site.url }}{{ post.url | xml_escape }}</loc>
      <lastmod>{{ post.date | date_to_xmlschema }}</lastmod>
    </url>
  {% endfor %}
</urlset>
